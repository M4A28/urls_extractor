{
    "manifest_version": 2,
    "name": "URL Extractor",
    "version": "1.0",
    "permissions": ["activeTab", "notifications"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
  }

  